mixin advantage(webp,img,alt,title,text)

   .advantage-wrapper 
      .advantage-box
         picture 
            source(type="image/webp", srcset="./img/" + webp)
            img.advantage-icon(src="./img/" + img, alt="" + alt, width="40", height="40", loading="lazy") 
      .advantage-container 
         h4.advantage-title #{title}
         p.advantage-text #{text}       